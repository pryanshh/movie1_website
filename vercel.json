{

    "builds": [{

        "src": "movie_website/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "movie_website/wsgi.py"
        }
    ]
}